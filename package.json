{
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:nuxt": "nuxi dev",
    "dev:graphql": "graphql-codegen --watch",
    "build": "run-s build:graphql build:nuxt",
    "build:nuxt": "nuxi build",
    "build:graphql": "graphql-codegen",
    "start": "cross-env NODE_ENV=production node .output/server/index.mjs",
    "deploy": "vercel",
    "fmt": "run-p 'fmt:*'",
    "fmt:eslint": "cross-env DEBUG=eslint:cli-engine eslint . --ext ts,js,vue --max-warnings=0 --fix",
    "fmt:prettier": "prettier . --write",
    "lint": "run-p 'lint:*'",
    "lint:eslint": "eslint . --ext ts,js,vue --max-warnings=0",
    "lint:prettier": "prettier . --check",
    "typecheck": "vue-tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.11",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@types/deep-equal": "^1.0.1",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^27.5.1",
    "@types/prettier": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "8.7.1",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "npm:nuxt3@3.0.0-rc.3-27550969.a4a3cff",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vercel": "^24.2.4",
    "vls": "^0.7.6",
    "vue-tsc": "^0.35.2"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.22.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@nuxtjs/web-vitals": "^0.1.8",
    "@sentry/tracing": "^7.0.0",
    "@sentry/vue": "^7.0.0",
    "@urql/core": "^2.5.0",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^0.1.7",
    "@urql/exchange-graphcache": "^4.4.1",
    "@urql/vue": "^0.6.2",
    "deep-equal": "^2.0.5",
    "graphql": "^16.5.0",
    "immer": "^9.0.14",
    "is-non-nullable": "^1.0.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.36"
  },
  "packageManager": "yarn@3.1.1"
}
