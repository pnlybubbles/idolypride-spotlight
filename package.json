{
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "deploy": "vercel",
    "fmt": "run-p 'fmt:*'",
    "fmt:eslint": "cross-env DEBUG=eslint:cli-engine eslint '**/*.{ts,js,vue}' --max-warnings=0 --fix",
    "fmt:prettier": "prettier '**/*.{json,yml}' --write --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit",
    "graphql": "graphql-codegen --config codegen.config.js"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.4",
    "@graphql-codegen/typescript": "2.4.4",
    "@graphql-codegen/typescript-operations": "^2.3.1",
    "@types/eslint": "^8.4.1",
    "@types/prettier": "^2.4.4",
    "@types/uuid": "^8.3.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "nuxt3": "^3.0.0-27415326.3c563fa",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "typescript": "^4.6.2",
    "vercel": "^24.0.0",
    "vls": "^0.7.6",
    "vue-tsc": "^0.33.2-patch.1"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.20.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "prerelease",
    "@sentry/tracing": "^6.18.2",
    "@sentry/vue": "^6.18.2",
    "@urql/exchange-auth": "^0.1.7",
    "@urql/vue": "^0.6.1",
    "graphql": "^16.3.0",
    "is-non-nullable": "^1.0.2",
    "uuid": "^8.3.2"
  },
  "packageManager": "yarn@3.1.1"
